{
  "name": "Google Meet Attendance ML",
  "version": "0.1.4",
  "manifest_version": 2,
  "description": "Google Meet Attendance Tracker with CSV Export",
  "homepage_url": "https://github.com/Mrgibsonict/Google-Meet-Attendance",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": ["js/inject.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Instructions Popup",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
  "tabs",
    "https://meet.google.com/*"
  ]
}